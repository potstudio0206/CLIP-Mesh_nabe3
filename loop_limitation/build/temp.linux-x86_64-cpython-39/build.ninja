ninja_required_version = 1.3
cxx = c++

cflags = -pthread -B /root/anaconda3/envs/clip-mesh/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /root/anaconda3/envs/clip-mesh/include -I/root/anaconda3/envs/clip-mesh/include -fPIC -O2 -isystem /root/anaconda3/envs/clip-mesh/include -fPIC -I/clipmesh/loop_limitation/thirdparty/eigen -I/clipmesh/loop_limitation/thirdparty/libigl/include -I/clipmesh/loop_limitation/src/loopEvaluation -I/root/anaconda3/envs/clip-mesh/lib/python3.9/site-packages/torch/include -I/root/anaconda3/envs/clip-mesh/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/clip-mesh/lib/python3.9/site-packages/torch/include/TH -I/root/anaconda3/envs/clip-mesh/lib/python3.9/site-packages/torch/include/THC -I/root/anaconda3/envs/clip-mesh/include/python3.9 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=loop_limitation -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc





build /clipmesh/loop_limitation/build/temp.linux-x86_64-cpython-39/./src/loop.o: compile /clipmesh/loop_limitation/src/loop.cpp
build /clipmesh/loop_limitation/build/temp.linux-x86_64-cpython-39/./src/loopEvaluation/LoopData.o: compile /clipmesh/loop_limitation/src/loopEvaluation/LoopData.cpp
build /clipmesh/loop_limitation/build/temp.linux-x86_64-cpython-39/./src/loopEvaluation/eigenStructure.o: compile /clipmesh/loop_limitation/src/loopEvaluation/eigenStructure.cpp
build /clipmesh/loop_limitation/build/temp.linux-x86_64-cpython-39/./src/loopEvaluation/limitationSurfaceComputation.o: compile /clipmesh/loop_limitation/src/loopEvaluation/limitationSurfaceComputation.cpp
build /clipmesh/loop_limitation/build/temp.linux-x86_64-cpython-39/./src/loopEvaluation/pointCollect.o: compile /clipmesh/loop_limitation/src/loopEvaluation/pointCollect.cpp
build /clipmesh/loop_limitation/build/temp.linux-x86_64-cpython-39/./src/loopEvaluation/pointEvaluate.o: compile /clipmesh/loop_limitation/src/loopEvaluation/pointEvaluate.cpp







